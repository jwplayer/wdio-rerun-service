{
  "name": "wdio-rerun-service",
  "version": "1.6.2",
  "description": "A WebdriverIO service to track and prepare for re-running failing or flaky Jasmine/Mocha tests or Cucumber Scenarios.",
  "author": "Mike Salvia <msalvia@jwplayer.com>",
  "homepage": "https://github.com/jwplayer/wdio-rerun-service",
  "license": "Apache-2.0",
  "main": "./build/index",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ../../babel.config.js",
    "watch": "npm-watch",
    "test": "run-s test:*",
    "test:clean": "rimraf ./results ./coverage rerun.sh",
    "test:eslint": "eslint src tests",
    "test:unit": "jest --collectCoverageFrom='[\"src/**/*.{js,jsx,ts,tsx}\"]' --coverage --collectCoverage=true --forceExit --detectOpenHandles tests/.*test.*"
  },
  "watch": {
    "build": "{src,tests}/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jwplayer/wdio-rerun-service.git"
  },
  "keywords": [
    "webdriver",
    "webdriverio",
    "wdio",
    "wdio-service",
    "rerun",
    "retry",
    "tests",
    "scenario",
    "cucumber",
    "mocha",
    "jasmine",
    "flaky",
    "selenium"
  ],
  "bugs": {
    "url": "https://github.com/jwplayer/wdio-rerun-service/issues"
  },
  "dependencies": {
    "@wdio/logger": "7.0.0",
    "minimist": "^1.2.5",
    "npm-watch": "^0.7.0",
    "uuid": "^8.3.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@typescript-eslint/eslint-plugin": "^4.16.0",
    "@typescript-eslint/parser": "^4.16.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-source-map-support": "^2.1.1",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.2"
  }
}
